{
  "name": "decky-wine-manager",
  "version": "0.0.1",
  "description": "A plugin used to managed your custom Wine / Proton installs",
  "scripts": {
    "build": "shx rm -rf dist && rollup -c",
    "watch": "rollup -c -w",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkyLeite/decky-wine-manager.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "steam-deck",
    "deck"
  ],
  "author": "Sky Leite <sky@leite.dev>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/SteamDeckHomebrew/decky-plugin-template/issues"
  },
  "homepage": "https://github.com/SteamDeckHomebrew/decky-plugin-template#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.0",
    "rollup": "^2.77.1",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "chrome-remote-interface": "^0.31.3"
  },
  "dependencies": {
    "decky-frontend-lib": "^3.6.1",
    "dotenv": "^16.0.3",
    "react-async-hook": "^4.0.0",
    "react-icons": "^4.4.0",
    "react-query": "^3.39.2",
    "rollup-plugin-import-css": "^3.1.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
